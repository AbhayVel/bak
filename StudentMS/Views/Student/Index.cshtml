@using StudentMS.Entities
@model List<Student>


    
 <table class="table">
     <tr>
         <td><a href="/student/index?columnName=id&orderBY=asc">Id</a></td>
         <td><a href="/student/index?columnName=FirstName&orderBY=asc">First Name</a></td>
         <td><a href="/student/index?columnName=LastName&orderBY=asc">Last Name:</a></td>
         <td><a href="/student/index?columnName=Class&orderBY=asc">Class</a></td>
         <td><a href="/student/index?columnName=Fees&orderBY=asc">Fees</a></td>
         <td>Age</td>
     </tr>

    @foreach(var item in Model)
    {
         <tr>
         <td>@item.Id</td>
         <td>@item.FirstName</td>
         <td>@item.LastName</td>
         <td>@item.Class</td>
         <td>@item.Fees</td>
         <td>@item.Age</td>
       </tr>
    }
   

    


 </table>